<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <item
        android:left="-2dp">
        <shape>
            <corners
                android:bottomRightRadius="4dp"
                android:topRightRadius="4dp" />
            <stroke
                android:width="1dp"
                android:color="@color/colorDottedLine"
                android:dashWidth="6dp"
                android:dashGap="3dp"/>
            <gradient
                android:angle="0"
                android:centerColor="#9DFF86"
                android:centerX="0.9"
                android:centerY="0.9"
                android:endColor="@color/colorLeanPageBackGround"
                android:startColor="#E2D2FF" />
        </shape>
    </item>
</layer-list>